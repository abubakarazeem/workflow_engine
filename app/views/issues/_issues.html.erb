<!-- Partial: Displays rows in a table -->
<% @issues.each do |issue| %>
  <tr>
    <td><%= link_to issue.project.title, issue_path(issue) %></td>
    <td><%= issue.issue_type.name %></td>
    <td><%= issue.title %></td>
    <td><%= issue.assignee.try(:name) %></td>  <!-- TODO:  add link_to after reverse merge-->
    <td><%= issue.issue_state.name %></td>
    <td><%= get_priority(issue.priority) %></td>
    <td><%= issue.start_date %></td>
    <td><%= issue.due_date %></td>
  </tr>
<% end %>