<% if current_user.admin? %>
  $('#create_watcher_<%= @watcher.try(:id) %>').fadeOut('slow');
<% end %>

$("#watch_button").remove();
$("#watch_button_div").append('<%= j render partial: "watch_button", locals: { issue: @issue } %>');
$("#issue_delete_button").data('confirm','<%= issue_delete_data_confirm(@issue) %>');
$("#flash_messages").html("<%= j render partial: 'shared/flash' %>");
