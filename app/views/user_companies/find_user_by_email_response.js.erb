<% if @found_users.count == 0 %>
  alert("Invalid Email.");
<% elsif @found_users.count == 1  %>
  $(location).attr("href", '<%= new_user_session_url(subdomain: @found_users.first.company.subdomain, user_email: @found_users.first.email) %>');
<% else %>
  $(location).attr("href", '<%= show_companies_path(user_email: @found_users.first.email) %>');
<% end %>
