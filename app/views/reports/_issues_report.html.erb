<tr>
  <td><%= link_to(issue.title, '#') %></td>
  <td><%= link_to(issue.project.title, project_path(issue.project)) %></td>
  <td><%= link_to(issue.assignee.full_name, member_path(issue.assignee)) %></td>
  <td><%= link_to(issue.creator.full_name, member_path(issue.creator)) %></td>
  <td><%= issue.issue_state.name %></td>
  <td><%= issue.issue_type.name %></td>
  <% if issue.priority > 0 %>
  <td><span class="badge badge-pill badge-danger"><%= issue_priority(issue) %></span></td>
  <% else %>
  <td><span class="badge badge-pill badge-success"><%= issue_priority(issue) %></span></td>
  <% end %>
  <td><%= issue.start_date %></td>
  <td><%= issue.due_date %></td>
</tr>

