$("#add_member_modal_flash").html('<%= j render partial: "project_memberships/notice" %>');
clearAddMemberModal();
<% unless @member.nil? %>
  <% if @member.class.name == 'User' %>
    $("#js_user_memberships").append('<%= j render partial: "member", locals: {member: @member} %>');
  <% else %>
    $("#js_team_memberships").append('<%= j render partial: "member", locals: {member: @member} %>');
  <% end %>
<% end %>
